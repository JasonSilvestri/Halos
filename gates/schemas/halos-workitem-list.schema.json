{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "halos-workitem-list.schema.json",
  "title": "Halos Work Item List",
  "type": "object",
  "properties": {
    "manifestId": {
      "type": "string"
    },
    "createdUtc": {
      "type": "string",
      "format": "date-time"
    },
    "workItems": {
      "type": "array",
      "items": {
        "$ref": "halos-workitem.schema.json"
      }
    }
  },
  "required": [
    "workItems"
  ],
  "additionalProperties": true
}